# Editor configuration, see https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[angular.json]
end_of_line = lf

[package.json]
end_of_line = lf

[*.bicep]
end_of_line = lf
